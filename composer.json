{
    "name": "ilzrv/fireable",
    "description": "An elegant way to trigger events based on attributes changes",
    "license": "MIT",
    "authors": [
        {
            "name": "Boris Lepikhin",
            "email": "info@envant.me",
            "homepage": "https://envant.me"
        }
    ],
    "homepage": "https://github.com/envant/fireable",
    "keywords": [
        "Laravel",
        "Fireable",
        "event"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0 || ^11.0",
        "illuminate/database": "^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1 || ^11.0.1",
        "orchestra/testbench": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Envant\\Fireable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Envant\\Fireable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
